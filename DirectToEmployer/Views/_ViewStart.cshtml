@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@{
    if (this.User.IsInRole("Jobseeker"))
    {
        Layout = "~/Views/Shared/_LayoutJobseeker.cshtml";
    }
    else if (this.User.IsInRole("Employer"))
    {
        Layout = "~/Views/Shared/_LayoutEmployer.cshtml";
    }
    else
    {
        Layout = "~/Views/Shared/_Layout.cshtml";
    }
}

